{
    "version":2,
    "builds":[
      {
        "src":"./index.js",
        "use" : "@vercel/node"
      }
    ],
    "routes": [
        {
          "src": "/users/(.*)",
          "dest": "/users/userRoutes.js"
        },
        {
          "src": "/products/(.*)",
          "dest": "/products/productRoutes.js"
        },
        {
          "src": "/orders/(.*)",
          "dest": "/orders/orderRoutes.js"
        },
        {
          "src": "/(.*)",
          "dest": "/index.js"
        }
      ]
  }